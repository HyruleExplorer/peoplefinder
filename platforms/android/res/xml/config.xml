<?xml version='1.0' encoding='utf-8'?>
<widget id="com.people.finder" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <feature name="Compass">
        <param name="android-package" value="org.apache.cordova.deviceorientation.CompassListener" />
    </feature>
    <name>PeopleFinder</name>
    <description>
        Incontra nuovi amici con PeopleFinder!.
    </description>
    <author email="vpervendetta1992@gmail.com" href="http://revandesigns.altervista.org">
        RevanDesigns - Progettazione web e mobile.
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Orientation" value="portrait" />
</widget>
